<!--Header Search Boxes-->
<div class="fmf-header-search container">
	<div class="gray-gradient radius-5 padding-10">
		<div class="row">
			<div class="col-sm-4">
				<input class="header-search-input margin-top-5" type="text" placeholder="Name, category..." />
			</div>
			<div class="col-sm-1 txt-center margin-top-10 white-text">
				NEAR
			</div>
			<div class="col-sm-5">
				<input class="header-search-input margin-top-5" type="text" placeholder="Location, neighborhood, etc..." />
			</div>
			<div class="col-sm-2 txt-center">
				<button class="fmf-ellipse-turquoise-button">Search</button>
			</div>
		</div>
	</div>

	<div class="margin-top-10 row">
		<div class="col-sm-1 bold">
			Categories:
		</div>
		<div class="col-sm-1 txt-center">
			<a href="#">Yoga</a>
		</div>
		<div class="col-sm-1 txt-center">
			<a href="#">Gyms</a>
		</div>
		<div class="col-sm-2 txt-center">
			<a href="#">Martial Arts</a>
		</div>
		<div class="col-sm-1 txt-center">
			<a href="#">Boxing</a>
		</div>
		<div class="col-sm-2 txt-center">
			<a href="#">Rock Climbing</a>
		</div>
		<div class="col-sm-1 txt-center">
			<a href="#">Pilates</a>
		</div>
		<div class="col-sm-2 txt-center">
			<a href="#">Dance</a>
		</div>
		<div class="col-sm-1 txt-center">
			<a href="#">Other</a>
		</div>
	</div>
</div>
<!--/Header Search Boxes-->

<!--Main-->
<div class="container margin-top-50 margin-bottom-50">
	<div class="row">
		<!--Main Column One-->
		<div class="col-sm-7">
			<div class="row">
				<div class="large-title col-sm-6">
					<%= @business_info.name %>
				</div>
				<div class="col-sm-6 txt-right">
					<button data-business="<%= @business_info.id %>" id="business-save-button" type="button" class="fmf-ellipse-dark-blue-button">+</button>
				</div>
			</div>

			<!--Gym Stars-->
			<div class="row">
				<div class="col-sm-12">
					<%= image_tag("stars/stars4.png") %>
				</div>
			</div>
			<!--/Gym Stars-->

			<div class="clearfix margin-top-20 gray-rollover-menu">
				<div id="toggle-bus-info" class="col-sm-4 gray-menu-item clickable txt-center gray-menu-item-active">
					INFO
				</div>
				<div id="toggle-bus-reviews" class="col-sm-4 gray-menu-item clickable txt-center">
					REVIEWS
				</div>
				<div id="toggle-bus-photos" class="col-sm-4 gray-menu-item clickable txt-center">
					PHOTOS
				</div>
			</div>

			<!--Info and Contact Info-->
			<div id="business-info" class="margin-top-20">
				<div class="row">
					<div class="col-sm-6">
						<div class="medium-title">
							<%= @business_info.address %>
						</div>
						<div class="medium-title">
							<%= @business_info.city %>, <%= @business_info.state %>
						</div>

						<div class="margin-top-20">
							<%= simple_format(@business_info.availability) %>
						</div>
						
						<div class="margin-top-20">
							<%= @business_info.phone %>
						</div>

						<div class="margin-top-20">
							<a href="<%= @business_info.website %>" target="_blank"><%= @business_info.website %></a>
						</div>

						<div class="margin-top-20 bold">
							Services:
						</div>
						<div class="row">
							<div class="col-sm-6">
								<% for i in 0..2 %>
									<div>
										<%= @services_info[i].try(:bus_service) %>
									</div>
								<% end %>
							</div>
							<div class="col-sm-6">
								<% for i in 3..5 %>
									<div>
										<%= @services_info[i].try(:bus_service) %>
									</div>
								<% end %>
							</div>
						</div>

						<div class="row">
							<div class="col-sm-6">
								<% for i in 6..8 %>
									<div>
										<%= @services_info[i].try(:bus_service) %>
									</div>
								<% end %>
							</div>
							<div class="col-sm-6">
								<% for i in 9..11 %>
									<div>
										<%= @services_info[i].try(:bus_service) %>
									</div>
								<% end %>
							</div>
						</div>

						<div class="row">
							<div class="col-sm-6">
								<% for i in 12..14 %>
									<div>
										<%= @services_info[i].try(:bus_service) %>
									</div>
								<% end %>
							</div>
							<div class="col-sm-6">
								<% for i in 15..17 %>
									<div>
										<%= @services_info[i].try(:bus_service) %>
									</div>
								<% end %>
							</div>
						</div>

						<div class="margin-top-50">
							<hr />
						</div>
					</div>
					<div class="col-sm-6">
						<div>
							<img src="http://placehold.it/300x300" />
						</div>
						<div>
							<button class="light-blue-large-button txt-center">
								Get directions <i class="fa fa-angle-double-right"></i>
							</button>
						</div>
					</div>
				</div>

				<div class="margin-top-20">
					<div class="medium-title">
						Description
					</div>
					<div class="margin-top-10">
						<%= @business_info.description %>
					</div>
				</div>

				<div class="margin-top-20">
					<button type="button" class="fmf-ellipse-dark-blue-button regular-text">Get deals now</button>
				</div>

				<div class="margin-top-20">
					<a href="#">
						<i class="fa fa-angle-double-left"></i> Go back to search
					</a>
				</div>
