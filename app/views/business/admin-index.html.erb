<!--Main-->
<input type="hidden" id="page_id" value="business_admin_index" />
<div class="container margin-top-20 margin-bottom-50">
	<div class="large-title">
		Select a Business to Manage
	</div>

	<!--One Business-->
	<% @businesses_owned.in_groups_of(4, false) do |group| %>
		<div class="row margin-top-20">
			<% group.each do |owned|
			business = get_bus_info(owned.business_id) %>
				<div class="col-sm-3">
					<div class="admin-select-business" style="background:url(<%= get_bus_thumb(business.business_hash, :link) %>) no-repeat;" data-id="<%= business.id %>">
						<div class="admin-select-business-label">
							<%= business.name %>
						</div>
					</div>
				</div>
			<% end %>
		</div>
	<% end %>
	<!--/One Business-->
</div>
<!--/Main-->